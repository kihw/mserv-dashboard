  "author": "mserv.wtf Team",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "check-disk-space": "^3.4.0",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "browserslist": [
    ">0.2%",
