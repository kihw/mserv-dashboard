{
  "name": "mserv-dashboard",
  "version": "2.1.0",
  "description": "Dashboard centralisé pour services auto-hébergés",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "vitest",
    "lint:fix": "eslint src --ext js --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "dashboard",
    "auto-hebergement",
    "services",
    "homelab"
  ],
  "author": "mserv.wtf Team",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}